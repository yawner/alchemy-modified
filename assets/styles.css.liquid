/* Medium and large screens */
.grid,
.row {
  display: flex;
  flex-wrap: wrap;
  margin-left: -{{ grid_gutter }}px;
}

.grid--row-gutters {
  margin-bottom: -{{ content_row_padding }}px;
}

.grid--row-gutters > .column {
  margin-bottom: {{ content_row_padding }}px;
}

.grid--row-gutters-small {
  margin-bottom: -{{ grid_gutter }}px;
}

.grid--row-gutters-small > .column {
  margin-bottom: {{ grid_gutter }}px;
}

.column {
  margin-left: {{ grid_gutter }}px;
}

.column.half {
  width: calc(50% - {{ grid_gutter }}px);
}

.column.quarter {
  width: calc(25% - {{ grid_gutter }}px);
}

.column.three-quarters {
  width: calc(75% - {{ grid_gutter }}px);
}

.column.third {
  width: calc(33.33333% - {{ grid_gutter }}px);
}

.column.two-thirds {
  width: calc(66.666667% - {{ grid_gutter }}px);
